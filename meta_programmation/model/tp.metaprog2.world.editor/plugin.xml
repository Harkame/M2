<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated world -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="tp.metaprog2.world.presentation.WorldModelWizardID"
            name="%_UI_WorldModelWizard_label"
            class="tp.metaprog2.world.presentation.WorldModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/WorldModelFile.gif">
         <description>%_UI_WorldModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated world -->
      <editor
            id="tp.metaprog2.world.presentation.WorldEditorID"
            name="%_UI_WorldEditor_label"
            icon="icons/full/obj16/WorldModelFile.gif"
            extensions="world"
            class="tp.metaprog2.world.presentation.WorldEditor"
            contributorClass="tp.metaprog2.world.presentation.WorldActionBarContributor">
      </editor>
   </extension>

</plugin>
