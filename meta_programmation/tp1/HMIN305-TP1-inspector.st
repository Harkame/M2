Object subclass: #MyInspector	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'HMIN305-TP1-inspector'!!MyInspector methodsFor: 'running' stamp: 'harkame 10/5/2018 10:17'!inspect: object	| attributes result |	result := ''.	attributes := object class allInstVarNames.	attributes do: [ :each | result := result, each, ' : ', (object instVarNamed: each) asString, '\'].	^ result withCRs.! !